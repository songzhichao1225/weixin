page {
  height: 100%;
}

.generalization {
  padding-bottom: 3rem;
}

text {
  display: block;
}

.generalization .mineIs .header {
  height: 464rpx;
  position: relative;
}

.generalization .mineIs .header image {
  width: 100%;
  height: 100%;
}

.generalization .mineIs .header .title {
  width: 80%;
  position: absolute;
  top: 20%;
  left: 10%;
}

.header .title .name {
  color: #fff;
  font-size: 50rpx;
}

.header .title text {
  font-size: 28rpx;
  color: #fff;
}

.header .title .btn {
  display: inline-block;
  width: 180rpx;
  height: 60rpx;
  font-size: 28rpx;
  background-color: #fff;
  color: #d85d27;
  text-align: center;
  line-height: 60rpx;
  margin-top: 20rpx;
}

.header .nav {
  position: absolute;
  height: 86rpx;
  width: 100%;
  bottom: 0;
  background: rgba(0, 0, 0, 0.7);
  display: flex;
  font-size: 14px;
  color: #adadad;
}

.header .nav .navSon {
  line-height: 86rpx;
  width: 25%;
  text-align: center;
}

.content {
  width: 100%;
}

.content .introductionSon {
  width: 85%;
  margin: 40rpx auto;
  overflow: hidden;
}

.content .introductionSon .img {
  width: 92rpx;
  height: 100rpx;
  display: block;
  float: left;
  margin: 98rpx 0 0 30rpx;
}

.content .introductionSon .text {
  width: 65%;
  float: right;
  margin-top: 30rpx;
}

.content .introduction .introductionSon .text text:first-child {
  font-size: 14px;
  color: #4a4a4a;
  font-weight: bold;
}

.content .introduction .introductionSon .text text:last-child {
  font-size: 12px;
  color: #4a4a4a;
  margin-top: 15rpx;
}

.content .introduction .join {
  width: 600rpx;
  height: 80rpx;
  background: #d85d27;
  border-radius: 21.5px;
  font-size: 18px;
  color: #fff;
  line-height: 80rpx;
  margin-top: 80rpx;
}

.Joined .banner {
  position: relative;
}

.Joined .banner text {
  display: block;
  text-align: center;
  color: #fff;
}

.Joined .banner image {
  display: block;
  width: 100%;
}

.Joined .banner .text {
  width: 60%;
  position: absolute;
  top: 56rpx;
  left: 20%;
}

.Joined .banner text:first-child {
  font-size: 24rpx;
}

.Joined .banner text:nth-child(2) {
  font-size: 48rpx;
  margin-top: 24rpx;
}

.Joined .banner text:nth-child(3) {
  font-size: 24rpx;
  margin-top: 24rpx;
}

.Joined .banner .order {
  margin-top: 36rpx;
}

.Joined .banner .order view {
  width: 50%;
  float: left;
}

.Joined .banner .order view:first-child text:first-child {
  font-size: 32rpx;
}

.Joined .banner .order view:first-child text:last-child {
  font-size: 24rpx;
}

.Joined .banner .order view:last-child text:first-child {
  font-size: 32rpx;
}

.Joined .banner .order view:last-child text:last-child {
  font-size: 24rpx;
}

.Joined .chart {
  width: 100%;
  height: 586rpx;
  overflow: hidden;
}

.container {
  width: 100%;
  height: 600rpx;
}

#column-dom {
  width: 100%;
  height: 560rpx;
}

.line {
  width: 2rpx;
  height: 24rpx;
  background: #d85d27;
  border-radius: 1px;
  margin-left: 5%;
  margin-top: 40rpx;
  float: left;
}

.title {
  float: left;
  display: block;
  font-size: 32rpx;
  color: #4a4a4a;
  margin-top: 30rpx;
  margin-left: 24rpx;
  font-weight: bold;
}

.chart .chartT {
  height: 500rpx;
  margin-top: -30rpx;
}

.chart .daysMonth {
  width: 180rpx;
  float: right;
  margin-top: 40rpx;
  margin-right: 5%;
}

.chart .daysMonth view {
  width: 60rpx;
  text-align: center;
  float: left;
  font-size: 12px;
  color: #9b9b9b;
}

.chart .money {
  clear: both;
  font-size: 10px;
  color: #4a4a4a;
  margin-top: 90rpx;
  margin-left: 5%;
}

.chart .chartT .days {
  width: 90rpx;
  display: block;
  font-size: 10px;
  color: #4a4a4a;
  margin-top: -30rpx;
  float: right;
  text-align: right;
  margin-right: 5%;
}

.mineNew .processed {
  width: 95%;
  height: 130rpx;
  background-image: linear-gradient(-135deg, #ff8b96 0%, #f12a48 100%);
  border-radius: 6px;
  clear: both;
  margin: 48rpx auto 0;
}

.mineNew .processed image {
  width: 64rpx;
  height: 62rpx;
  margin: 34rpx 0 0 32rpx;
  float: left;
}

.mineNew .processed .content {
  display: block;
  width: 250rpx;
  height: 100%;
  font-size: 14px;
  color: #fff;
  float: left;
  text-align: center;
  line-height: 130rpx;
}

.mineNew .processed .rightText {
  width: 140rpx;
  height: 100%;
  font-size: 20px;
  color: #fff;
  float: right;
  display: block;
  text-align: center;
  line-height: 130rpx;
}

.mineNew .address {
  background-image: linear-gradient(-135deg, #ccff94 0%, #61b006 100%);
  border-radius: 6px;
}

.stadium {
  margin-top: 80rpx;
}

.look {
  width: 140rpx;
  font-size: 12px;
  color: #9b9b9b;
  float: right;
  margin-right: 5%;
  margin-top: 40rpx;
  text-align: right;
}

.stadium .stadiumList {
  width: 90%;
  height: 136rpx;
  margin: 40rpx auto;
  clear: both;
}

.stadium .stadiumList .stadiumImg {
  width: 176rpx;
  height: 100%;
  float: left;
}

.stadium .stadiumList .content {
  float: left;
  width: 70%;
  height: 136rpx;
  margin-left: 24rpx;
  position: relative;
}

.stadium .stadiumList .content .name {
  font-size: 14px;
  color: #333;
}

.stadium .stadiumList .content .type {
  font-size: 12px;
  color: #d85d27;
}

.stadium .stadiumList .content  .distance {
  font-size: 12px;
  color: #9b9b9b;
  position: absolute;
  right: 0;
  bottom: 0;
}

.prompt {
  width: 90%;
  margin: 40rpx auto;
}

.prompt image {
  width: 24rpx;
  height: 22rpx;
  display: block;
  float: left;
}

.prompt text {
  width: 93%;
  font-size: 12px;
  color: #4a4a4a;
  float: left;
  margin-left: 2%;
}

.correctRate {
  width: 160rpx;
  height: 62rpx;
  background: #fff;
  border-radius: 90px 0 0 90px;
  position: absolute;
  top: 24rpx;
  right: 0;
  padding-left: 6rpx;
}

.correctRate text {
  display: block;
  font-size: 10px;
  color: #d85d27;
  text-align: center;
  margin-top: 4rpx;
}

.praise {
  height: 50rpx;
  margin-top: 10rpx;
}
.praise image:first-child{
  margin-left: 0;
}
.praise image {
  display: block;
  width: 0.9rem;
  height: 0.9rem;
  float: left;
  margin-left: 0.2rem;
}

.lookof {
  font-size: 14px;
  color: #9b9b9b;
}
