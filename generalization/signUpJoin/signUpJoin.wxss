.signUpJoin {
  height: 100%;
  border-top: 0.06rem solid #E9E9E9;
}

.list {
  padding: 0.94rem 0.75rem;
  border-bottom: 0.06rem solid #E9E9E9;
  font-size: 0.88rem;
  overflow: hidden;
}

.list>text {
  float: left;
}

.switch {
  float: right;
  margin-top: -0.4rem;
}

.input {
  float: right;
  text-align: right;
  color: #9B9B9B;
}

.picker {
  color: #9b9b9b;
  float: right;
}

.list>image {
  width: 0.44rem;
  height: 0.69rem;
  float: right;
  margin-top: 0.2rem;
  margin-left: 0.5rem;
}

.list .map {
  float: right;
  color: #9B9B9B;
  max-width: 63%;
  text-align: right;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap
}

.list .mapDing {
  width: 1.3rem;
  height: 1.3rem;
  float: right;
  margin-top: -0.1rem;
}

.weui-cell__hd {
  float: right;
}

.weui-cell__bd {
  float: left;
}

.weui-cell {
  display: block;
  overflow: hidden;
  border-bottom: 0.06rem solid #e9e9e9;
  padding: 0.3rem 0;
}

.head {
  padding: 0.5rem 5%;
  overflow: hidden;
  border-bottom: 0.06rem solid #e9e9e9;
}

.head text:first-child {
  float: left;
}

.head text:last-child {
  float: right;
}

.btnBotm {
  width: 80%;
  height: 2.69rem;
  color: #fff;
  background: #D85D27;
  border-radius: 1.35rem;
  margin: 3rem auto 2rem;
}

.btn {
  width: 80%;
  padding: 20rpx 0;
  border-radius: 10rpx;
  text-align: center;
  margin: 40rpx 10%;
  background: #0C1939;
  color: #fff;
}

/*mask*/
.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
}

/*content*/
.drawer_attr_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 1001;
  background: #fff;
}

.drawer_content {
  padding: 20rpx 40rpx;
  height: 550rpx;
  overflow-y: scroll;
}

.drawer_title {
  padding: 20rpx;
  font: 42rpx "microsoft yahei";
  text-align: center;
}

.line {
  border-bottom: 1px solid #f8f8f8;
}