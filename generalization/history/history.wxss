.history {
  width: 95%;
  margin: 0 auto;
}
.history .title{
  overflow: hidden;
}
.history .title image {
  width: 76rpx;
  height: 76rpx;
  border-radius: 50%;
  float: left;
}

.history .title  .information {
  float: left;
  margin-left: 16rpx;
}

.history .title  .information text:first-child {
  font-size: 14px;
  color: #4a4a4a;
  display: block;
}

.history .title  .information text:last-child {
  font-size: 12px;
  color: #9b9b9b;
  display: block;
}

.history .content {
  font-size: 14px;
  color: #4a4a4a;
  margin-top: 16rpx;
  display: block;
}
.history .imgT{
  width: 168rpx;
  height: 168rpx;
}


