.yesResults {
  height: 100%;
  border-top: 0.06rem solid #e9e9e9;
}

.yesResults .projectNav {
  width: 95%;
  height: 2rem;
  margin: 0 auto;
  display: flex;
  border-bottom: 0.06rem solid #e9e9e9;
}

.yesResults .projectNav>view {
  flex: 1;
  position: relative;
}

.yesResults .projectNav>view>text {
  font-size: 14px;
  display: block;
  text-align: center;
  line-height: 2rem;
}

.yesResults .projectNav>view>image {
  width: 0.4rem;
  height: 0.3rem;
  position: absolute;
  right: 0;
  top: 0.9rem;
}

.mask {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  position: absolute;
  top: 2rem;
  z-index: 100;
}

.mask .activity {
  width: 25%;
  height: 21.1rem;
  background-color: #fff;
}

.mask .activity .activitySon {
  width: 70%;
  height: 2.3rem;
  margin: 0 auto;
}

.mask .activity .activitySon text {
  font-size: 12px;
  color: #333;
  line-height: 2.3rem;
}

.mask .activity .activitySon image {
  width: 0.3rem;
  height: 0.4rem;
  display: block;
  float: right;
  margin-top: 1rem;
}

.mask .activityTwo {
  width: 25%;
  position: absolute;
  top: 0;
  left: 25%;
  background-color: #fff;
}

.mask .activityTwo .activityTwoSon text {
  font-size: 12px;
  color: #9b9b9b;
  display: block;
  text-align: center;
  height: 2rem;
  line-height: 2rem;
}

.mask .sort {
  width: 25%;
  position: absolute;
  top: 0;
  left: 25%;
  background-color: #fff;
}

.mask .sort text {
  font-size: 12px;
  color: #9b9b9b;
  display: block;
  text-align: center;
  height: 2rem;
  line-height: 2rem;
}

.projectSon {
  width: 100%;
  height: 8.8rem;
  position: relative;
  border-bottom: 1px solid #ccc;
}

.projectSon .avatar {
  width: 5rem;
  float: left;
  margin-left: 0.8rem;
}

.projectSon .avatar .avatarP {
  width: 5rem;
  height: 5rem;
  border-radius: 10px;
  margin-top: 1.2rem;
}

.projectSon .avatar .aIcon {
  width: 44rpx;
  height: 40rpx;
  margin-left: 0.3rem;
  margin-top: 0.2rem;
  display: block;
  float: left;
}

.projectSon .avatar text {
  display: block;
  padding: 0 0.3rem;
  height: 0.8rem;
  font-size: 10px;
  color: #fff;
  text-align: center;
  line-height: 0.8rem;
  background: linear-gradient(270deg, #FCA400 0%, #FEE073 100%);
  border-radius: 2px;
  float: left;
  margin-left: 0.4rem;
  margin-top: 0.4rem;
}

.projectSon .introduction {
  width: 66%;
  float: left;
  overflow: hidden;
  margin-top: 1.2rem;
  margin-left: 0.9rem;
}

.projectSon .introduction .title {
  overflow: hidden;
}

.projectSon .introduction .title text {
  font-size: 0.88rem;
  color: #333;
  display: block;
  float: left;
  font-weight: bold;
}

.projectSon .introduction .title image {
  width: 0.6rem;
  height: 0.6rem;
  display: block;
  float: left;
  margin: 0.25rem 0 0 0.2rem;
}

.projectSon .introduction .mode {
  overflow: hidden;
}

.projectSon .introduction .mode text {
  display: block;
  font-size: 14px;
  color: #4a4a4a;
  margin-left: 0.5rem;
  float: left;
}

.projectSon .introduction .time {
  overflow: hidden;
}

.projectSon .introduction .time text {
  display: block;
  font-size: 14px;
  color: #4a4a4a;
  margin-left: 0.5rem;
  float: left;
}

.projectSon .introduction .address {
  overflow: hidden;
}

.projectSon .introduction .address text {
  display: block;
  font-size: 14px;
  color: #4a4a4a;
  margin-left: 0.5rem;
  float: left;
}

.projectSon .introduction .cost {
  overflow: hidden;
}

.projectSon .introduction .cost text {
  display: block;
  font-size: 14px;
  color: #4a4a4a;
  margin-left: 0.3rem;
  float: left;
}

.projectSon .introduction .cost text:last-child {
  float: right;
}

.projectSon .status {
  width: 94rpx;
  height: 84rpx;
  position: absolute;
  top: 0;
  right: 0;
  z-index: 99;
}

.projectSon .status image {
  width: 100%;
  height: 100%;
  display: block;
}

.noContent {
  padding-top: 30%;
  width: 14.12rem;
  margin: 0 auto;
}

.noContent>image {
  width: 100%;
  height: 10.5rem;
}

.noContent>text {
  font-size: 0.75rem;
  display: block;
  text-align: center;
}