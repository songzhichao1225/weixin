.createSuccess {
  width: 100%;
}

.banner {
  width: 100%;
  height: 10.88rem;
  position: relative;
}

.banner .image {
  width: 100%;
  height: 100%;
}

.banner .imageS {
  width: 3.88rem;
  height: 3.88rem;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -1.94rem;
  margin-left: -1.94rem;
}

.ojbk {
  display: block;
  width: 100%;
  text-align: center;
  color: #fff;
  position: absolute;
  bottom: 15%;
}

.text {
  position: relative;
}

.headTop {
  width: 80%;
  background: #FFFFFF;
  box-shadow: 0.13rem 0.19rem 0.69rem 0rem rgba(225, 225, 225, 0.5);
  border-radius: 0.5rem;
  margin-left: 7%;
  margin-top: -1rem;
  padding: 1rem 0.63rem;
}

.headTop>text {
  display: block;
  text-align: center;
}

.headTop>.comm {
  font-size: 0.88rem;
  color: #D0021B;
  display: block;
  margin-top: 0.5rem;
}

.center {
  width: 95%;
  margin: 1.88rem auto 0;
}

.center>text {
  display: block;
}

.center>text:first-child {
  font-size: 0.75rem;
  color: #D85D27;
}
.btn{
  margin-top: 1.31rem;
}

.btn>button:first-child {
  width: 95%;
  height: 2.69rem;
  background: #D85D27;
  border-radius: 1.34rem;
  color: #fff;
}

.btn>button:last-child {
  width: 95%;
  height: 2.69rem;
  border: 0.06rem solid #D85D27;
  border-radius: 1.34rem;
  color: #D85D27;
  background: #fff;
  margin-top: 1rem;
}