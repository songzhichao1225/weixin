.createSuccess {
  width: 100%;
}

.banner {
  width: 100%;
  height: 10.88rem;
  position: relative;
}

.banner .image {
  width: 100%;
  height: 100%;
}

.banner .imageS {
  width: 3.88rem;
  height: 3.88rem;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -1.94rem;
  margin-left: -1.94rem;
}

.text {
  margin-top: 2rem;
}

.text text {
  display: block;
  font-size: 1rem;
  text-align: center;
}

.text .comm {
  display: block;
  width: 95%;
  font-size: 0.8rem;
  margin: 1rem auto 0;
  text-align: center;
}

.title {
  font-size: 0.88rem;
  display: block;
  text-align: center;
  margin-top: 2rem;
}

.title text {
  padding-left: 0.8rem;
  font-size: 0.75rem;
  color: #9B9B9B;
}

.btn {
  overflow: hidden;
  width: 80%;
  margin: 3rem auto 0;
}

.btn .btnSonOne {
  width: 40%;
  height: 2.13rem;
  border-radius: 0.38rem;
  border: 0.06rem solid rgba(216, 93, 39, 1);
  float: left;
  text-align: center;
  line-height: 2.13rem;
  color: #D85D27;
  font-size: 0.75rem;
}

.btn .btnSonOne:last-child {
  margin-left: 18.5%;
}