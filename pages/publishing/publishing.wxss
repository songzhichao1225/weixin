  line-height: 1rem;
  margin-top: 0.9rem;
  font-size: 0.75rem;
  color: #9b9b9b;
}

.zhezhao {
  width: 100%;
  height: 100%;
  background: rgba(000, 000, 000, .5);
  position: absolute;
  z-index: 999;
  top: 0;
}

.zhezhao .sonBox {
  width: 80%;
  margin: 40% auto 1rem;
  background: #fff;
  border-radius: 0.5rem;
  padding-bottom: 0.75rem;
}

.zhezhao .close {
  width: 2.31rem;
  height: 2.31rem;
  margin: 0 auto;
}

.zhezhao .close>image {
  width: 100%;
  height: 100%
}

.zhezhao .title {
  font-size: 0.88rem;
  text-align: center;
  padding-top: 0.75rem;
  padding-bottom: 0.75rem;
  font-weight: bold;
}

.zhezhao .text {
  width: 90%;
  margin: 0 auto;
  font-size: 0.75rem;
  display: block;
}

.botmComfire {
  font-size: 0.75rem;
  text-align: right;
  padding-right: 0.5rem;
  border-top: 0.06rem solid #e9e9e9;
  padding: 0.5rem 0;
}

.botmComfire text {
  padding-right: 0.2rem;
}

.masking {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  position: absolute;
  top: 0;
}

.masking .content {
  width: 80%;
  margin: 40% auto 0;
  padding: 1rem 0;
  background: #fff;
  border-radius: 0.5rem;
}

.close {
  width: 2rem;
  height: 2rem;
  margin: 5% auto 0;
}

.close image {
  width: 100%;
  height: 100%;
}

.masking .content .title {
  font-size: 0.88rem;
  font-weight: bold;
  text-align: center;
}

.masking .content .son {
  width: 90%;
  margin: 0.3rem auto 0;
  font-size: 0.75rem;
}

.masking .content .son>text:first-child {
  font-weight: bold;
}

.sanList {
  color: #9b9b9b;
}

.sanList>text {
  padding-right: 0.5rem;
  color: #D85D27;
}

.forbade {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1001;
}

.forbade .contentbade {
  width: 85%;
  min-height: 2rem;
  border-radius: 0.79rem;
  background: #fff;
  position: absolute;
  top: 10%;
  left: 7.5%;
