  line-height: 22rpx;
  color: #fff;
  float: left;
  margin-left: 12rpx;
}

.content .peopleList .son .TeamAson {
  width: 100%;
  height: 30rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  color: #fff;
  text-align: center;
  line-height: 30rpx;
  font-size: 12px;
  background: rgba(74, 74, 74, 1);
  border-radius: 0rem 0rem 0.19rem 0.19rem;
