.publishing {
  height: 100%;
}

.publishing .line {
  height: 0.63rem;
  background: rgba(243, 243, 243, 1);
}

.navTitle {
  overflow: hidden;
}

.navTitle>view {
  width: 50%;
  height: 2.81rem;
  text-align: center;
  float: left;
  color: #333;
  font-size: 1rem;
  line-height: 2.5rem;
}
.navTitle>view .lineText{
  width: 1rem;
  height: 0.18rem;
  background: #D85D27;
  margin:0 auto;
}

.publishing .lineTwo {
  height: 0.31rem;
  background: rgba(243, 243, 243, 1);
}

.content .list {
  height: 2.75rem;
  line-height: 2.75rem;
  font-size: 0.88rem;
  border-bottom: 0.06rem solid #e9e9e9;
}

.content .list .image {
  width: 0.44rem;
  height: 0.69rem;
  float: right;
  margin-top: 1.03rem;
  margin-right: 0.69rem;
}

.content .list .textTwo {
  float: right;
  padding-right: 0.3rem;
  color: #9b9b9b;
  display: block;
  width: 3rem;
  text-align: center;
}

.content .list .weui-input {
  width: 5rem;
  float: left;
  height: 2.75rem;
  padding-left: 0.2rem;
}

.content .joinTil {
  height: 2.88rem;
  font-size: 1rem;
  font-weight: 600;
  border-bottom: 0.06rem solid #e9e9e9;
  line-height: 2.88rem;
  padding-left: 0.75rem;
}

.content .aList {
  width: 95%;
  margin: 0.6rem auto;
}

.content .aList .titleSon {
  font-size: 1.06rem;
  width: 3rem;
  float: left;
  text-align: center;
}

.content .TeamA {
  margin-top: 0.81rem;
  overflow: hidden;
  padding-bottom: 1.56rem;
  border-bottom: 0.06rem solid #e9e9e9;
}

.content .TeamA text {
  font-size: 17px;
  color: #333;
  float: left;
  margin-top: 26rpx;
}

.content .TeamB {
  margin-top: 0.81rem;
  overflow: hidden;
  padding-bottom: 1.56rem;
  border-bottom: 0.06rem solid #e9e9e9;
}

.content .TeamB text {
  font-size: 17px;
  color: #333;
  float: left;
  margin-top: 26rpx;
}

.content .peopleList {
  width: 538rpx;
  float: left;
  margin-left: 50rpx;
}

.content .peopleList .son:nth-child(3N+1) {
  margin-left: 0;
}

.content .peopleList .son {
  width: 157.8rpx;
  height: 86rpx;
  float: left;
  margin-left: 26rpx;
  position: relative;
  margin-top: 26rpx;
}

.content .peopleList .son .logo {
  width: 87.2rpx;
  height: 87.2rpx;
  display: block;
  float: left;
  border-radius: 6rpx;
}

.content .peopleList .son .project {
  width: 44.6rpx;
  height: 39.8rpx;
  display: block;
  float: left;
  margin-left: 16.2rpx;
  margin-top: 12rpx;
}

.content .peopleList .son .grade {
  width: 52.6rpx;
  height: 22rpx;
  background: #f5a623;
  border-radius: 4rpx;
  font-size: 20rpx;
  margin-top: 12.2rpx;
  text-align: center;
  line-height: 22rpx;
  color: #fff;
  float: left;
  margin-left: 12rpx;
}

.content .peopleList .son .TeamAson {
  width: 100%;
  height: 30rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  color: #fff;
  text-align: center;
  line-height: 30rpx;
  font-size: 12px;
  background:rgba(74,74,74,1);
border-radius:0rem 0rem 0.19rem 0.19rem;
opacity:0.5;
}

.content .peopleList .son .TeamAsonTwo {
  width: 87.2rpx;
  height: 30rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  color: #fff;
  text-align: center;
  line-height: 30rpx;
  font-size: 12px;
  background:rgba(74,74,74,1);
  border-radius:0rem 0rem 0.19rem 0.19rem;
  opacity:0.5;
}

.btnConfirm {
  width: 80%;
  height: 2.5rem;
  background: rgba(216, 93, 39, 1);
  border-radius: 1.34rem;
  font-size: 1.06rem;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: rgba(255, 255, 255, 1);
  line-height: 2.5rem;
  text-align: center;
  margin: 2rem auto 0;
}

.invite {
  width: 87.2rpx;
  height: 87.2rpx;
  font-size: 12px;
  line-height: 80rpx;
  text-align: center;
  position: relative;
}

.invite image {
  width: 100%;
  height: 100%;
  position: absolute;
}
