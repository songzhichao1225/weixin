}

.content .peopleList .son {
  width: 157.8rpx;
  height: 86rpx;
  float: left;
  margin-left: 26rpx;
  position: relative;
  margin-top: 26rpx;
}

.content .peopleList .son .logo {
  width: 87.2rpx;
  height: 87.2rpx;
  display: block;
