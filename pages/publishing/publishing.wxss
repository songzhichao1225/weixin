page{
  height: 100%;
}
.publishing {
  height: 100%;
  position: relative
}

.publishing .line {
  height: 0.63rem;
  background: rgba(243, 243, 243, 1);
}

.navTitle {
  overflow: hidden;
  
}
.textPianhao{
  font-size: 0.65rem;
  display: block;
  line-height: 0rem;
  margin-top: -0.3rem;
}

.navTitle>view {
  width: 40%;
  height: 2.81rem;
  text-align: center;
  float: left;
  color: #333;
  font-size: 1rem;
  line-height: 2.5rem;
}
.navTitle>view .lineText{
  width: 1rem;
  height: 0.18rem;
  background: #D85D27;
  margin:0 auto;
}

.publishing .lineTwo {
  height: 0.31rem;
  background: rgba(243, 243, 243, 1);
}

.content .list {
  min-height: 2.75rem;
  line-height: 2.75rem;
  font-size: 0.88rem;
  border-bottom: 0.06rem solid #e9e9e9;
  overflow: hidden;
}
.content .list .left{
  width: 52%;
  float: left;
}
.content .list .right{
  width: 44%;
  float: right;
}

.content .list .image {
  width: 0.44rem;
  height: 0.69rem;
  float: right;
  margin-top: 1.03rem;
  margin-right: 0.69rem;
}

.content .list .textTwo {
  float: right;
  color: #9b9b9b;
  display: block;
  width: 3rem;
  text-align: center;
}

.content .list .weui-input {
  width: 5rem;
  float: left;
  height: 2.75rem;
  padding-left: 0.2rem;
}

.content .joinTil {
  height: 2.88rem;
  font-size: 1rem;
  font-weight: 600;
  border-bottom: 0.06rem solid #e9e9e9;
  line-height: 2.88rem;
  padding-left: 0.75rem;
}

.content .aList {
  width: 95%;
  margin: 0.6rem auto;
}

.content .aList .titleSon {
  font-size: 1.06rem;
  width: 3rem;
  float: left;
  text-align: center;
}

.content .TeamA {
  margin-top: 0.81rem;
  overflow: hidden;
  padding-bottom: 1.56rem;
  border-bottom: 0.06rem solid #e9e9e9;
}

.content .TeamA text {
  font-size: 17px;
  color: #333;
  float: left;
  margin-top: 26rpx;
}

.content .TeamB {
  margin-top: 0.81rem;
  overflow: hidden;
  padding-bottom: 1.56rem;
}

.content .TeamB text {
  font-size: 17px;
  color: #333;
  float: left;
  margin-top: 26rpx;
}

.content .peopleList {
  width: 538rpx;
  float: left;
  margin-left: 50rpx;
}

.content .peopleList .son:nth-child(3N+1) {
  margin-left: 0;
}

.content .peopleList .son {
  width: 157.8rpx;
  height: 86rpx;
  float: left;
  margin-left: 26rpx;
  position: relative;
  margin-top: 26rpx;
}

.content .peopleList .son .logo {
  width: 87.2rpx;
  height: 87.2rpx;
  display: block;
  float: left;
  border-radius: 6rpx;
}

.content .peopleList .son .project {
  width: 44.6rpx;
  height: 44.6rpx;
  display: block;
  float: left;
  margin-left: 16.2rpx;
  margin-top: 12rpx;
}

.content .peopleList .son .grade {
  width: 52.6rpx;
  height: 22rpx;
  background: #f5a623;
  border-radius: 4rpx;
  font-size: 20rpx;
  margin-top: 12.2rpx;
  text-align: center;
  line-height: 22rpx;
  color: #fff;
  float: left;
  margin-left: 12rpx;
}

.content .peopleList .son .TeamAson {
  width: 100%;
  height: 30rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  color: #fff;
  text-align: center;
  line-height: 30rpx;
  font-size: 12px;
  background:rgba(74,74,74,1);
  border-radius:0rem 0rem 0.19rem 0.19rem;
  opacity:0.5;
}

.content .peopleList .son .TeamAsonTwo {
  width: 87.2rpx;
  height: 30rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  color: #fff;
  text-align: center;
  line-height: 30rpx;
  font-size: 12px;
  border-radius:0rem 0rem 0.19rem 0.19rem;
  background:rgba(74,74,74,0.5);
}

.btnConfirm {
  width: 80%;
  height: 2.5rem;
  background: rgba(216, 93, 39, 1);
  border-radius: 1.34rem;
  font-size: 1.06rem;
  font-family: PingFangSC-Semibold, PingFang SC;
  color: rgba(255, 255, 255, 1);
  line-height: 2.5rem;
  text-align: center;
  margin: 2rem auto 0;
}

.invite {
  width: 87.2rpx;
  height: 87.2rpx;
  font-size: 12px;
  line-height: 80rpx;
  text-align: center;
  position: relative;
}

.invite image {
  width: 100%;
  height: 100%;
  position: absolute;
}
.textarea{
  width: 75%;
  float: right;
  line-height: 1rem;
  margin-top: 0.9rem;
  font-size: 0.75rem;
  color: #9b9b9b;
}
.zhezhao{
  width: 100%;
  height: 100%;
  background: rgba(000, 000, 000,.5);
  position: absolute;
  z-index: 999;
  top: 0;
}
.zhezhao .sonBox{
  width: 80%;
  margin: 40% auto 1rem;
  background: #fff;
  border-radius: 0.5rem;
  padding-bottom: 0.75rem;
}
.zhezhao .close{
  width: 2.31rem;
  height: 2.31rem;
  margin: 0 auto;
}
.zhezhao .close>image{
  width: 100%;
  height: 100%
}
.zhezhao  .title{
  font-size: 0.88rem;
  text-align: center;
  padding-top: 0.75rem;
  padding-bottom: 0.75rem;
  font-weight: bold;
}
.zhezhao .text{
  width: 90%;
  margin: 0 auto;
  font-size: 0.75rem;
  display: block;
}
.botmComfire{
  font-size: 0.75rem;
  text-align: right;
  padding-right: 0.5rem;
  border-top: 0.06rem solid #e9e9e9;
  padding: 0.5rem 0;
}
.botmComfire text{
  padding-right: 0.2rem;
}
.masking{
  width: 100%;
  height: 100%;
  background:rgba(0, 0, 0, 0.5);
  position: absolute;
  top: 0;
}
.masking .content{
  width: 80%;
  margin: 40% auto 0;
  padding: 1rem 0;
  background: #fff;
  border-radius: 0.5rem;
}
.close{
  width: 2rem;
  height: 2rem;
  margin: 5% auto 0;
}
.close image{
  width: 100%;
  height: 100%;
}
.masking .content .title{
  font-size: 0.88rem;
  font-weight: bold;
  text-align: center;
}
.masking .content .son{
  width: 90%;
  margin: 0.3rem auto 0;
  font-size: 0.75rem;
}
.masking .content .son>text:first-child{
  font-weight: bold;
}
.sanList{
  float: right;
  color: #9b9b9b;
}
.sanList>text{
  padding-right: 0.5rem;
  color: #D85D27;
}

.forbade{
  width: 100%;
  height: 100%; 
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1001;
}
.forbade .contentbade{
  width: 85%;
  min-height: 2rem;
  border-radius: 0.79rem;
  background: #fff;
  position: absolute;
  top: 10%;
  left: 7.5%;
  padding: 1.56rem 0;
}
.forbade .contentbade>text:first-child{
  display: block;
  text-align: center;
  font-size: 1rem;
}
.forbade .contentbade .twoCode{
  width: 13.38rem;
  height: 13.88rem;
  display: block;
  margin: 0 auto;
}
.forbade .contentbade>text:nth-child(3){
  font-size: 1rem;
  display: block;
  text-align: center;
  color: #D85D27;
}
.closeTwo{
  display: block;
  width: 1.5rem;
  height: 1.5rem;
  position: absolute;
  top: 0;
  right: 0;
}
