.patterns text {
  font-size: 0.75rem;
  padding-left: 0.5rem;
  display: block;
  float: left;
}

.patterns text:first-child {
  color: #D85D27;
  padding-left: 0;
}

.rightImg {
  width: 2.94rem;
  height: 2.94rem;
  position: absolute;
  right: 0;
  top: 0rem;
}

.rightImg>image {
  width: 100%;
  height: 100%;
}

.bosBoxBtn {
  width: 5.06rem;
  height: 1.88rem;
  background: linear-gradient(90deg, #FF8522 0%, #F8350D 100%);
  border-radius: 0.94rem;
  text-align:center;
  line-height: 1.88rem;
  position: fixed;
  right: 2.5%;
  margin-top: -0.3rem;
}
.buchangjin{
  font-size: 0.75rem;
  margin-top: 2rem;
  
  overflow: hidden;
}
.buchangjin>radio{
  float: left;
  transform: scale(0.8);
}
.buchangjin>text{
  float: left;
  line-height: 1.5rem;
}
.compensation {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(000, 000, 000, 0.7);
}

.kokao {
  width: 80%;
  height: auto;
  background: #fff;
  border-radius: 1rem;
  margin: 10% auto 0;
}

.kokao .headFire {
  display: block;
  font-size: 1rem;
  text-align: center;
  line-height: 2.5rem;
}
.kokao .contentTxt{
  font-size: 0.88rem;
  display: block;
  width: 90%;
  margin: 0 auto;
  line-height: 1.5rem;
  color: #9b9b9b;
}
.kokao .footet{
  height: 3rem;
  border-top: 0.06rem solid #ccc;
  line-height: 3rem;
  text-align: center;
  font-size: 0.88rem;
  margin-top: 1rem;
  color: rgb(37, 123, 235);
}
.jieguoTxt{
  display: block;
  width: 95%;
  margin: 0 auto;
  font-size: 0.75rem;
  color:#9b9b9b ;
}
.xushi{
  width: 2.5rem;
  background: #D85D27;
  font-size: 0.75rem;
  text-align: center;
  color: #fff;
  margin-left: 0.5rem;
  border-radius: 0.3rem;
}


.certificate{
  width: 100%;
  height: 100%;
  background: rgba(000, 000, 000, 0.7 );
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1001;
}
.certificate>image:first-child{
   width: 100%;
   height: 24.49rem;
   position: absolute;
   top: 50%;
   margin-top: -12.245rem;
}

.certificate>image:last-child{
  width: 2.06rem;
  height: 2.06rem;
  top: 50%;
  left: 50%;
  position: absolute;
  margin-top: 12.245rem;
  margin-left: -1.03rem;
}