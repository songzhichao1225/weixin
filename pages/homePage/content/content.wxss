/**index.wxss**/

page {
  height: 100%;
}

.content {
  margin: 0;
  color: #4a4a4a;
  overflow: hidden;
  min-height: 110%;
  position: relative;
}

.isSignN {
  width: 750rpx;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.7);
  z-index: 99;
}

.isSignN .isSignbox {
  width: 676rpx;
  height: 544rpx;
  position: absolute;
  z-index: 999;
  left: 37rpx;
  top: 128rpx;
}

.isSignN .isSignbox .img {
  width: 100%;
  height: 100%;
}

.isSignbox .txt {
  display: block;
  position: absolute;
  top: 350rpx;
  font-size: 16px;
  color: #000;
  left: 200rpx;
}

.isSignN .close {
  width: 66rpx;
  height: 66rpx;
  margin: 750rpx auto 0;

}

.isSignN .close image {
  width: 100%;
  height: 100%;
}

.content .headerCity {
  width: 100%;
  margin: 0 auto;
  overflow: hidden;
  background: #F5520B;
}

.inputBox {
  width: 52%;
  margin-left: 0.7rem;
  height: 1.8rem;
  float: left;
  border-radius: 200px;
  background: #fff;
  margin-top: 0.2rem;
}

.content .headerCity text {
  width: 15%;
  font-size: 14px;
  display: block;
  text-align: center;
  float: left;
  height: 2.3rem;
  line-height: 2.3rem;
  color: #fff;
}

.content .news {
  width: 1.25rem;
  height: 1.25rem;
  margin-top: 0.5rem;
  margin-left: 0.5rem;
}

.content .headerCity .image {
  display: block;
  width: 0.7rem;
  height: 0.4rem;
  float: left;
  margin-top: 1rem;
}


.content .headerCity input {
  display: block;
  float: left;
  width: 50%;
  height: 1.8rem;
  margin-left: 0.2rem;
  font-size: 14px;
}

.content .bannerBack .back {
  width: 100%;
  height: 6rem;
}

.content .banner {
  width: 90%;
  height: 7.25rem;
  margin: -6.2rem auto;
}

.content .banner image {
  width: 100%;
  height: 100%;
  border-radius: 0.75rem;
}

.content .homeSelect {
  width: 100%;
  margin: 6.1rem auto 0;
  display: flex;
  flex-wrap:wrap;
  border-bottom: 0.06rem solid #ccc;
  padding-bottom: 0.5rem;
}

.content .homeSelect view {
  width: 25%;
  margin-top: 0.5rem;
}


.content .homeSelect view>image {
  width: 2.3rem;
  height: 2.3rem;
  display: block;
  margin: 0 auto;
}

.content .homeSelect view>text {
  font-size: 0.75rem;
  color: #333;
  text-align: center;
  display: block;
  margin-top: 0.5rem;
}

.content .projectNav {
  width: 95%;
  height: 1.3rem;
  margin: 0 auto;
  display: flex;
  padding-top: 0.5rem;
}

.content .projectNav>view {
  flex: 1.3;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}
.content .projectNav>view:nth-child(2){
  flex: 1.3;
  display: flex;
  justify-content: center;
  align-items: center;
}  
.content .projectNav>view:first-child{
  flex: 1.3;
  display: flex;
  justify-content: center;
  align-items: center;
}
.content .projectNav>view:last-child{
  flex: 1.1;
  display: flex;
  justify-content: center;
  align-items: center;
}

.content .projectNav>view>text {
  font-size: 14px;
  display: block;
  text-align: center;
  line-height: 1.3rem;
  float: left;
}

.content .imgRight{
  width: 0.4rem;
  height: 0.3rem;
   float: left;
   margin-top: 0.2rem;
}

.content .projectList {
  height: 50%;
  min-height: 980rpx;
  overflow: hidden;
  position: relative;
  margin-top: 0.5rem;
  padding-bottom: 2.5rem;
}

.content .projectList .projectSon {
  width: 100%;
  height: 8rem;
  position: relative;
  border-top: 1px solid #ccc;
}

.content .projectList .projectSon .avatar {
  width: 5rem;
  float: left;
  margin-left: 0.8rem;
}

.content .projectList .projectSon .avatar .avatarP {
  width: 5rem;
  height: 5rem;
  border-radius: 10px;
  margin-top: 0.5rem;
}

.content .projectList .projectSon .avatar .aIcon {
  width: 35rpx;
  height: 35rpx;
  margin-left: 0.3rem;
  margin-top: 0.2rem;
  display: block;
  float: left;
}

.content .projectList .projectSon .avatar text {
  display: block;
  padding: 0 0.3rem;
  height: 0.8rem;
  font-size: 10px;
  color: #fff;
  text-align: center;
  line-height: 0.8rem;
  background: linear-gradient(270deg, #FCA400 0%, #FEE073 100%);
  border-radius: 2px;
  float: left;
  margin-left: 0.4rem;
  margin-top: 0.4rem;
}

.content .projectList .projectSon .introduction {
  width: 66%;
  float: left;
  overflow: hidden;
  margin-top: 0.5rem;
  margin-left: 0.9rem;
}

.content .projectList .projectSon .introduction .title {
  overflow: hidden;
}

.content .projectList .projectSon .introduction .title text {
  font-size: 0.88rem;
  color: #333;
  display: block;
  float: left;
  font-weight: bold;
}

.content .projectList .projectSon .introduction .title image {
  width: 0.94rem;
  height: 0.94rem;
  display: block;
  float: left;
  margin: 0.1rem 0 0 0.2rem;
}

.content .projectList .projectSon .introduction .mode {
  overflow: hidden;
}

.content .projectList .projectSon .introduction .mode text {
  display: block;
  font-size: 14px;
  color: #4a4a4a;
  margin-left: 0.5rem;
  float: left;
}

.content .projectList .projectSon .introduction .time {
  overflow: hidden;
}

.content .projectList .projectSon .introduction .time text {
  display: block;
  font-size: 14px;
  color: #4a4a4a;
  margin-left: 0.5rem;
  float: left;
}

.content .projectList .projectSon .introduction .address {
  overflow: hidden;
}

.content .projectList .projectSon .introduction .address text {
  display: block;
  font-size: 14px;
  color: #4a4a4a;
  margin-left: 0.5rem;
  float: left;
}

.content .projectList .projectSon .introduction .cost {
  overflow: hidden;
}

.content .projectList .projectSon .introduction .cost text {
  display: block;
  font-size: 14px;
  color: #4a4a4a;
  margin-left: 0.3rem;
  float: left;
}

.content .projectList .projectSon .introduction .cost text:last-child {
  float: right;
}

.content .projectList .projectSon .status {
  width: 94rpx;
  height: 84rpx;
  position: absolute;
  top: 0;
  right: 0;
  z-index: 99;
}

.content .projectList .projectSon .status image {
  width: 100%;
  height: 100%;
  display: block;
}

.model {
  width: 100%;
  height: 2rem;
  display: block;
  text-align: center;
  font-size: 16px;
  position: absolute;
  top: 1rem;
}

.mask {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  position: absolute;
  top: 0;
  z-index: 100;
}

.mask .activity {
  width: 26%;
  height: 17.1rem;
  background-color: #fff;
}

.mask .activity .activitySon {
  width: 70%;
  height: 2.3rem;
  margin: 0 auto;
}

.mask .activity .activitySon text {
  font-size: 12px;
  color: #333;
  line-height: 2.3rem;
}

.mask .activity .activitySon image {
  width: 0.3rem;
  height: 0.4rem;
  display: block;
  float: right;
  margin-top: 1rem;
}

.mask .activityTwo {
  width: 32.5%;
  position: absolute;
  top: 0;
  left: 25%;
  background-color: #fff;
}

.mask .activityTwo .activityTwoSon text {
  font-size: 12px;
  color: #9b9b9b;
  display: block;
  text-align: center;
  height: 2rem;
  line-height: 2rem;
}

.mask .sort {
  width: 25%;
  position: absolute;
  top: 0;
  left: 26%;
  background-color: #fff;
}

.mask .sort text {
  font-size: 12px;
  color: #9b9b9b;
  display: block;
  text-align: center;
  height: 2rem;
  line-height: 2rem;
}

.flexd {
  width: 108rpx;
  height: 108rpx;
  position: fixed;
  right: 24rpx;
  bottom: 70rpx;
  z-index: 999;
}

.flexd image {
  width: 100%;
  height: 100%;
}
.noContent{
  width: 80%;
  margin:5rem auto 0;
}
.noContent image{
  width: 9rem;
  height: 6.5rem;
  margin: 0 auto;
  display: block;
}
.noContent text{
  display: block;
  text-align: center;
  font-size: 0.75rem;
}
.bossZhe{
  width: 100%;
  height: 100%;
  position: absolute;
  background: rgba(000, 000, 000, 0.6);
  top:0;
  left:0;
  z-index: 1001;

}
.bossZhe>.shoModel{
  width: 65%;
  height: 7rem;
  background: #fff;
  border-radius: 0.75rem;
  position: absolute;
  top: 30%;
  left: 17.5%;
  padding:0 2.5%;
  font-size: 0.88rem;
}
.bossZhe>.shoModel>text{
  display: block;
  margin-top: 1rem;
  text-align: center;
}
.bossZhe>.shoModel .footerBtn{
  border-top: 0.06rem solid #e9e9e9;
  margin-top: 1rem;
}
.bossZhe>.shoModel .footerBtn>.right{
  border-left: 0.06rem solid #e9e9e9;
}
.bossZhe>.shoModel .footerBtn>view{
  width: 49%;
  height:2.5rem;
  float: left;
  text-align: center;
  line-height: 2.5rem;
  position: relative;
}
.bossZhe>.shoModel .footerBtn button{
  border:none;
  background: #fff;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
}
.forbade{
  width: 100%;
  height: 100%; 
  background:rgba(000, 000, 000, 0.7);
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1001;
}
.forbade .contentbade{
  width: 85%;
  min-height: 2rem;
  border-radius: 0.79rem;
  background: #fff;
  position: absolute;
  top: 10%;
  left: 7.5%;
  padding: 1.56rem 0;
}
.forbade .contentbade>text:first-child{
  display: block;
  text-align: center;
  font-size: 1rem;
}
.forbade .contentbade .twoCode{
  width: 13.38rem;
  height: 13.88rem;
  display: block;
  margin: 0 auto;
}
.forbade .contentbade>text:nth-child(3){
  font-size: 1rem;
  display: block;
  text-align: center;
  color: #D85D27;
}
.closeTwo{
  display: block;
  width: 1.5rem;
  height: 1.5rem;
  position: absolute;
  top: 0;
  right: 0;
}
.customizing{
  height: 3rem;
  background:#F5520B ;
}
.scroll{
  height: 100%;
  overflow-y: auto;
}